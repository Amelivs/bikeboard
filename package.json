{
  "name": "bikeboard",
  "version": "2022.10.2",
  "author": "Guillaume Erhart",
  "homepage": "https://github.com/Amelivs/bikeboard",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:sw": "ng build --configuration development && http-server -p 4200 -c-1 www",
    "build": "ng build --configuration production",
    "splash": "pwa-asset-generator src/assets/icons/icon-512.png src/assets/splash --splash-only --portrait-only --padding 40% --quality 90 --index src/index.html",
    "deploy": "npm run build && npx firebase deploy",
    "deploy:alpha": "npm run build && npx firebase hosting:channel:deploy alpha",
    "prebuild": "node tools/version.js",
    "lint": "ng lint --fix",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.4",
    "@angular/common": "^14.2.4",
    "@angular/compiler": "^14.2.4",
    "@angular/core": "^14.2.4",
    "@angular/forms": "^14.2.4",
    "@angular/platform-browser": "^14.2.4",
    "@angular/platform-browser-dynamic": "^14.2.4",
    "@angular/router": "^14.2.4",
    "@angular/service-worker": "^14.2.4",
    "@ionic/angular": "^6.2.9",
    "@ionic/storage-angular": "^3.0.6",
    "ol": "^7.1.0",
    "pwa-asset-generator": "^6.1.3",
    "rxjs": "~7.5.7",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.4",
    "@angular-eslint/builder": "14.1.2",
    "@angular-eslint/eslint-plugin": "14.1.2",
    "@angular-eslint/eslint-plugin-template": "14.1.2",
    "@angular-eslint/schematics": "14.1.2",
    "@angular-eslint/template-parser": "14.1.2",
    "@angular/cli": "^14.2.4",
    "@angular/compiler-cli": "^14.2.4",
    "@types/jasmine": "~3.10.6",
    "@types/node": "^16.11.47",
    "@types/webcrypto": "^0.0.28",
    "@typescript-eslint/eslint-plugin": "5.38.1",
    "@typescript-eslint/parser": "5.38.1",
    "eslint": "^8.24.0",
    "eslint-plugin-filename-rules": "^1.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-unused-imports": "^2.0.0",
    "firebase-tools": "^11.13.0",
    "http-server": "^14.1.1",
    "jasmine-core": "~4.0.1",
    "karma": "~6.3.20",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.1",
    "karma-jasmine": "~4.0.2",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.6.4"
  }
}
