{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/*.css",
          "/*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/icons/**",
          "/assets/audio/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    },
    {
      "name": "icons",
      "installMode": "prefetch",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/svg/layers-outline.svg",
          "/svg/navigate-outline.svg",
          "/svg/navigate.svg",
          "/svg/compass.svg",
          "/svg/notifications.svg",
          "/svg/close.svg"
        ]
      }
    },
    {
      "name": "traces",
      "installMode": "lazy",
      "updateMode": "lazy",
      "resources": {
        "files": [
          "/assets/gpx/**"
        ]
      }
    },
    {
      "name": "openstreetmap",
      "resources": {
        "urls": [
          "https://*.tile.openstreetmap.fr/osmfr/**",
          "https://*.tile-cyclosm.openstreetmap.fr/cyclosm/**"
        ]
      }
    }
  ]
}